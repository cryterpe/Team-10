{
    "name": "quickAdd to Google Calendar",
    "version": "1.1",
    "description": "Add calender events using a pop-up without needing to open
    a Google Calendar webpage. Comes with date, time, location, event name,
    and description input options. ",
    "permissions":["activeTab","declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": { /* need to fix images for icon */
          "16": "IEEE-icon-16-px.ppp",
          "32": "IEEE-icon-32-px.ppp",
          "48": "IEEE-icon-48-px.ppp",
          "128": "IEEE-icon.ppp"
        }
    },
    "options_page": "options.html",
    "manifest_version": 2
  }
