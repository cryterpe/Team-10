{
    "name": "quickUCSD",
    "version": "1.1",
    "description": "Provides quick details on personalized student data about 
    classes schedule. Comes with links to commonly used UCSD sites. ",
    "permissions":["activeTab","declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/IEEE-icon-16-px.png",
          "32": "images/IEEE-icon-32-px.png",
          "48": "images/IEEE-icon-48-px.png",
          "128": "images/IEEE-icon.png"
        }
    },
    "options_page": "options.html",
    "manifest_version": 2
  }
